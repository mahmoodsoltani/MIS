                      dbo.Pm_Post ON dbo.Pm_Lab_Rol_Tamirat.Srl_Pm_Post = dbo.Pm_Post.Srl LEFT OUTER JOIN
                      dbo.Pm_TajhizName ON dbo.Pm_TajhizName.Srl = dbo.Pm_Hadese_KhesaratDevice.Srl_pm_TajhizName

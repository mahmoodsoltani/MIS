                      dbo.Pm_Lab_Rol_Tamirat.Srl_Pm_Hadese_KhesaratDevice
FROM         dbo.Pm_Lab_Rol_Tamirat LEFT JOIN
                      dbo.Pm_Lab_Rol_SatheAhamniat ON dbo.Pm_Lab_Rol_Tamirat.Srl_Pm_Lab_Rol_SatheAhamniat = dbo.Pm_Lab_Rol_SatheAhamniat.Srl  LEFT JOIN

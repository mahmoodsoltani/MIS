                    <CommandText>SELECT        Pm_Hadese.Srl, Pm_Hadese.ReportNo AS [شماره گزارش],
 Pm_Hadese.Tarikh AS تاریخ,
 Pm_Hadese_Elat.Elat AS [علت بروز حادثه], 
 Pm_Group.Name AS [نام گروه],dbo.Pm_Vahed.Name as واحد,
pm_ashkhas.name+' '+pm_ashkhas.family as [ثبت کننده],
isnull(case  when not Pm_Hadese.Srl_Pm_Post is null then
 pm_post.name 
 else ''
   end,'') as [محل وقوع],
 isnull(case  when not Pm_Hadese.Srl_Pm_Line is null then
	pm_line.name 
 else ''
 end ,'' )as [نام خط]
FROM    Pm_Hadese INNER JOIN
         Pm_Hadese_Elat ON Pm_Hadese.Srl_PM_Hadese_Elat = Pm_Hadese_Elat.Srl LEFT JOIN
          Pm_Post ON Pm_Hadese.Srl_Pm_Post = Pm_Post.Srl 
          LEFT JOIN pm_line  on Srl_Pm_Line = pm_line.srl   LEFT JOIN         Pm_Group ON Pm_Hadese.Srl_Pm_Group = Pm_Group.Srl LEFT JOIN
          Pm_Vahed ON Pm_Hadese.Srl_Pm_Vahed = Pm_Vahed.Srl LEFT JOIN pm_ashkhas
On pm_ashkhas.srl=pm_hadese.srl_submituser 

WHERE     (dbo.Pm_Hadese.Srl = @Srl)

  </CommandText>

              <Mapping SourceColumn="واحد" DataSetColumn="واحد" />
            </Mappings>
            <Sources>
              <DbSource ConnectionRef="Nama_PMConnectionString (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillBySearchOption" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetDataBySearchOption" GeneratorSourceName="FillBySearchOption" GetMethodModifier="Public" GetMethodName="GetDataBySearchOption" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataBySearchOption" UserSourceName="FillBySearchOption">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Pm_Hadese.Srl, Pm_Hadese.ReportNo AS [شماره گزارش],
 Pm_Hadese.Tarikh AS تاریخ,
 Pm_Hadese_Elat.Elat AS [علت بروز حادثه], 
 Pm_Group.Name AS [نام گروه],dbo.Pm_Vahed.Name as واحد,
pm_ashkhas.name+' '+pm_ashkhas.family as [ثبت کننده],
isnull(case  when not Pm_Hadese.Srl_Pm_Post is null then
 pm_post.name 
 else ''
   end,'') as [محل وقوع],
 isnull(case  when not Pm_Hadese.Srl_Pm_Line is null then
	pm_line.name 
 else ''
 end ,'' )as [نام خط]
FROM    Pm_Hadese INNER JOIN
         Pm_Hadese_Elat ON Pm_Hadese.Srl_PM_Hadese_Elat = Pm_Hadese_Elat.Srl LEFT JOIN
          Pm_Post ON Pm_Hadese.Srl_Pm_Post = Pm_Post.Srl 
          LEFT JOIN pm_line  on Srl_Pm_Line = pm_line.srl  LEFT JOIN         Pm_Group ON Pm_Hadese.Srl_Pm_Group = Pm_Group.Srl LEFT JOIN
          Pm_Vahed ON Pm_Hadese.Srl_Pm_Vahed = Pm_Vahed.Srl LEFT JOIN pm_ashkhas
On pm_ashkhas.srl=pm_hadese.srl_submituser 
WHERE        
(Pm_Hadese.ReportNo = @ReportNo or @ReportNo=-1) 
AND  
(Pm_Hadese.Tarikh &gt;= @begin or @begin='-1')  AND (Pm_Hadese.Tarikh &lt;= @end or @end='-1') AND 
(Pm_Hadese.Srl_PM_Hadese_Elat = @Srl_PM_Hadese_Elat or @Srl_PM_Hadese_Elat=-1) AND                      
(Pm_Hadese.Srl_Pm_Hadese_SharayetBoroz = @Srl_Pm_Hadese_SharayetBoroz or @Srl_Pm_Hadese_SharayetBoroz=-1) AND 
(Pm_Hadese.Srl_Pm_Post = @Srl_Pm_Post or @Srl_Pm_Post=-1)AND 
(Pm_Hadese.Srl_Pm_Line=@Srl_Pm_Line or @Srl_Pm_Line =-1) AND

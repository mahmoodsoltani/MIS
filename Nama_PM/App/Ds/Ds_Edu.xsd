          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Pm_Edu_ِDoreReportTableAdapter" GeneratorDataComponentClassName="Pm_Edu_ِDoreReportTableAdapter" Name="Pm_Edu_ِDoreReport" UserDataComponentName="Pm_Edu_ِDoreReportTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="Nama_PMConnectionString (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select distinct Pm_Edu_DoreHolding.Srl,
	StartDate as شروع , EndDate as پایان,
	Pm_Edu_DoreHolding.[Length] as طول, Pm_Edu_Teacher .Name+' '+Pm_Edu_Teacher.Family  مدرس,
	Pm_Edu_DorePresenter.Name as [ارائه کننده],
	Pm_Edu_DorePlace .Name as محل,
	Edu_Price+Refahi_Price+Other_Price as هزینه,
	case when Place=1 then 'داخل استان'
		when Place=2 then 'خارج استان'
		when Place=3 then 'خارج کشور'
		End as موقعیت 
                ,pm_edu_dore.name as دوره
from Pm_Edu_DoreHolding join Pm_Edu_Dore 
on Srl_Dore = Pm_Edu_Dore.Srl join Pm_Edu_DorePlace 
on Srl_Place  = Pm_Edu_DorePlace.Srl join Pm_Edu_DorePresenter 
on Srl_Presenter  = Pm_Edu_DorePresenter.Srl   join Pm_Edu_Teacher 
on Srl_Teacher = Pm_Edu_Teacher.Srl left join Pm_Edu_DoreParticipation 
on Pm_Edu_DoreParticipation.Srl_DoreHolding = Pm_Edu_DoreHolding.Srl
where (StartDate &gt;= @StartDate or @StartDate='-1')
and	  (StartDate &lt;=@EndDate or @EndDate ='-1')
and   (Srl_Place = @Srl_Place or @Srl_Place=-1)
and   (Srl_Presenter = @Srl_Presenter or @Srl_Presenter=-1)

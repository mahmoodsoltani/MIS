                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select  Pm_Edu_DoreHolding.Srl , 
Pm_Edu_Dore .Name as عنوان,
StartDate as شروع,
EndDate as پایان,
Pm_Edu_DoreHolding .[Length] as مدت,
Pm_Edu_Teacher.Name+' '+Pm_Edu_Teacher.Family as مدرس,
Pm_Edu_DorePlace .Name as محل,
Pm_Edu_DorePresenter .Name  as برگزارکننده	,
Edu_Price as [هزینه آموزشی],
Refahi_Price as [هزینه رفاهی],
Other_Price as [هزینه متفرقه],
Edu_Price+Refahi_Price+Other_Price  as [هزینه کل],
case when 
	Place=1 then 'داخل استان' 
	when Place =2 then 'خارح استان'
	when Place = 3 then 'خارج کشور'
end 
as مکان , SessionCount  as جلسات
 from Pm_Edu_DoreHolding join Pm_Edu_Dore on Pm_Edu_Dore.Srl = Srl_Dore 
left join Pm_Edu_DorePlace on Srl_Place = Pm_Edu_DorePlace .Srl
left join Pm_Edu_DorePresenter  on Srl_Presenter= Pm_Edu_DorePresenter .Srl
left join Pm_Edu_Teacher on Srl_Teacher = Pm_Edu_Teacher .Srl

</CommandText>

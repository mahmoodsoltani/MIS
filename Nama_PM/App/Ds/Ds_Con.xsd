 [نام خط],
t3.نوع,
t3.Srl,Srl_vahed,Srl_group,srl_post,  srl_line,
ISNULL(is_cancle,0)as is_cancle,ISNULL(is_night,0)as is_night,
isnull(is_holiday,0)as is_holiday from (
SELECT        Pm_Con_StatusStatement.Number شماره	, Pm_Con_StatusStatement.Tarikh تاریخ,isnull(Pm_Vahed .Name,'') [نام واحد],
Pm_Group .Name as [نام گروه],isnull(pm_post.Name,'') [نام پست],
isnull(Pm_Line  .Name,'') as [نام خط],
'غیر فرم کار' as نوع,
 Pm_Con_StatusStatement.Srl,
 Pm_Con_StatusStatement.Srl_vahed,
 Pm_Con_StatusStatement.Srl_group,ISNULL( Pm_Con_StatusStatement.Srl_post,0) srl_post, 
                        ISNULL( Pm_Con_StatusStatement.Srl_Line,0) srl_line

FROM            Pm_Con_StatusStatement INNER JOIN
                         Pm_Vahed ON Pm_Con_StatusStatement.Srl_vahed = Pm_Vahed.Srl INNER JOIN
                         Pm_Group ON Pm_Con_StatusStatement.Srl_group = Pm_Group.Srl left JOIN
                         Pm_Post ON Pm_Con_StatusStatement.Srl_post = Pm_Post.Srl left JOIN
                         Pm_Line ON Pm_Con_StatusStatement.Srl_Line = Pm_Line.Srl
union                  
SELECT        Pm_WorkForm_Master.No شماره	, Pm_WorkForm_Master.WorkFormTarikh تاریخ, Pm_Vahed .Name [نام واحد],
Pm_Group .Name as [نام گروه],
pm_post.Name [نام پست],
'' as [نام خط],
'فرم کار' as نوع,
 Pm_WorkForm_Master.Srl,
Pm_WorkForm_Master.Srl_pm_vahed as srl_vahed, Pm_WorkForm_Master.Srl_pm_group as srl_group,
 Pm_WorkForm_Master.Srl_Pm_Post_From as srl_post, 
                         0 as srl_line
FROM            Pm_WorkForm_Master  INNER JOIN
                         Pm_Vahed ON Pm_WorkForm_Master.Srl_Pm_Vahed = Pm_Vahed.Srl INNER JOIN
                         Pm_Group ON Pm_WorkForm_Master.Srl_Pm_Group = Pm_Group.Srl left JOIN
                         Pm_Post ON Pm_WorkForm_Master.Srl_Pm_Post_From = Pm_Post.Srl 
where Pm_WorkForm_Master .srl in (select Srl_workform_master from Pm_Con_StatusStatementItem )
)as t3 left join (
select distinct is_holiday, is_night, is_cancle,
case when Srl_StatusStatement IS null then Srl_workform_master 
else Srl_StatusStatement end as srl,case when Srl_StatusStatement IS null then 'فرم کار'
else 'غیر فرم کار' end as نوع from Pm_Con_StatusStatementItem ) as t2 
on (t2.srl = t3.Srl and t2.[نوع] = t3.[نوع] )
where (Srl_vahed = @Srl_Vahed or @Srl_Vahed =-1)
and (Srl_group = @srl_group or @srl_group= -1)
and (srl_line =@srl_line or @srl_line =  -1)
and (srl_post = @srl_post or @srl_post =-1)
and ([تاریخ] &gt;= @StartDate or @StartDate='-1')
and ([تاریخ] &lt;= @EndDate or @EndDate='-1')

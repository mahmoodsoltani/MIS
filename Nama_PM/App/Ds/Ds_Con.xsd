left join Pm_Line on Srl_Line = Pm_Line .Srl 
join (select * from Pm_Con_ContractItems   where 
     srl_contract in (SELECT *  FROM SPLIT(@srl_contract , ',') )  or @srl_contract='-1'  ) as pm_con_contractitem
     on pm_con_contractitem.Srl = srl_contractItem 

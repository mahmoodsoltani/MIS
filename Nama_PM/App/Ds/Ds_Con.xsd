left join Pm_Con_Source on Pm_Con_Source.Srl = srl_source
left join Pm_PostType on Pm_PostType.Srl = srl_Omoor 
where 
(_Month in(SELECT *  FROM SPLIT(@srl_Month , ',') )  or @srl_Month='-1')

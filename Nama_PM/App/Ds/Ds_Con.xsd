            <MainSource>
              <DbSource ConnectionRef="Nama_PMConnectionString (MySettings)" DbObjectName="SELECT CASE WHEN Pm_Con_StatusStatement.Tarikh IS NULL THEN Pm_WorkForm_Master.WorkFormTarikh ELSE Pm_Con_StatusStatement .Tarikh END AS tarikh, CASE WHEN Srl_workform_master IS NULL THEN CASE WHEN Pm_Con_StatusStatement .Srl_post IS NULL THEN Pm_Con_StatusStatement.Srl_Line ELSE Pm_Con_StatusStatement.Srl_post END ELSE Pm_WorkForm_Master.Srl_Pm_Post_From END AS srl_Post, pm_vahed.Srl AS srlvahed,Pm_Vahed .Name , Pm_Con_Item .sstype ,CASE WHEN Srl_workform_master IS NULL THEN 0 ELSE 1 END AS isworkform, Pm_Con_Item .Code ,Pm_Con_Item .Is_starred,pm_con_item.Title ,Pm_Con_ContractItems .Amount , Pm_Con_ContractItems .Price , CASE WHEN Pm_WorkForm_Master.Srl IS NULL AND Pm_Con_StatusStatement .Srl IS NULL THEN 0 ELSE Pm_Con_StatusStatementItem .Amount END AS DoAmount, Pm_Con_Item .Srl AS SrlItem ,Pm_Con_Item.Code AS ItemCode,Pm_Con_Item.Title AS ItemTitle FROM (SELECT * FROM Pm_Con_ContractItems WHERE Pm_Con_ContractItems .Srl_Contract =@Contract )Pm_Con_ContractItems LEFT JOIN Pm_Con_StatusStatementItem ON Pm_Con_ContractItems .srl=srl_contractItem JOIN Pm_Con_Item ON Pm_Con_Item .Srl = Srl_Item AND Pm_Con_Item .Srl_Vahed = @vahed AND (Pm_Con_Item .sstype = @sstype OR @sstype=-1) AND (Pm_Con_Item .Is_starred = @star OR @star=-1) LEFT JOIN Pm_WorkForm_Master ON Pm_Con_StatusStatementItem .Srl_workform_master = Pm_WorkForm_Master .srl AND NOT Srl_workform_master IS NULL AND (Pm_WorkForm_Master.Srl_Pm_Post_From=@post OR @post=-1) AND (NOT Srl_workform_master IS NULL AND (Pm_WorkForm_Master .WorkFormTarikh &gt;=@StartDate OR @StartDate='-1')) AND (NOT Srl_workform_master IS NULL AND (Pm_WorkForm_Master .WorkFormTarikh &lt;=@EndDate OR @EndDate='-1')) AND (@isWorkform=1) LEFT JOIN Pm_Con_StatusStatement ON Pm_Con_StatusStatementItem.Srl_StatusStatement = Pm_Con_StatusStatement .Srl AND ((Srl_workform_master IS NULL AND Pm_Con_StatusStatement .Srl_Line IS NULL AND (Pm_Con_StatusStatement .Srl_post =@post OR @post=-1) ) OR(Srl_workform_master IS NULL AND Pm_Con_StatusStatement .Srl_post IS NULL AND (Pm_Con_StatusStatement .Srl_Line =@post OR @post=-1))) AND (Srl_workform_master IS NULL AND (Pm_Con_StatusStatement .Tarikh &gt;=@StartDate OR @StartDate='-1')) AND (Srl_workform_master IS NULL AND (Pm_Con_StatusStatement .Tarikh &lt;=@EndDate OR @EndDate='-1')) AND (@isStatusStatement=1) LEFT JOIN Pm_Vahed ON (Pm_Vahed.Srl = Pm_Con_StatusStatement .Srl_vahed OR Pm_Vahed.Srl= Pm_WorkForm_Master.Srl_Pm_Vahed ) AND pm_vahed.Srl = @vahed" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">

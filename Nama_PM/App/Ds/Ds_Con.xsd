                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--exec pm_con_Excelrp 97,1,3,1400,1,1,'-1','-1',-1,215,-1,9,1,30
SELECT [Pm_Con_MonthReport].[Srl]
      ,[Number] شماره,
[letternumber] as [شماره نامه]

      ,convert(nvarchar,[Month])+'_'+cast([Year]as nvarchar(10))+'_'+Pm_Month.Name  دوره
      ,case when pm_postType.name IS null then
		pm_con_source.name
		else 
		pm_con_source.name+'_'+pm_postType.name
		end as گروه
,pm_vahed.name [نام واحد]
      ,Pm_con_statusStatementtype .name نوع
      ,[SendDate] [تاریخ ارسال]
      ,convert(bigint,[Price]) مبلغ
      ,[ConfirmDate] [تاریخ تائید]
      ,convert(bigint,[ConfirmPrice]) [مبلغ تائید]
      ,[ERPDate] [تاریخ ERP]
      ,[DeliveryNumber] [شماره پیگیری]
      ,[StatusNumber] [شماره وضعیت]
      ,[DocumentNumber] [شماره سند]
      ,[Pm_Con_MonthReport].[Description] توضیحات
      ,pm_con_contract.Description as قرارداد
      ,[IsCancle] 
      ,[Srl_Con_Contract]
      ,[Srl_Pm_Con_source]
      ,[Srl_Pm_PostType]
      ,[Srl_Pm_Vahed]
      ,[Pm_Con_MonthReport].[Srl_SubmitUser]
      ,[Pm_Con_MonthReport].[SubmitDate]
,srl_Statusstatementtype, [Month]
  FROM [Nama_MIS].[dbo].[Pm_Con_MonthReport]
join pm_vahed on pm_vahed.srl = [Pm_Con_MonthReport].srl_pm_vahed
join pm_con_source on  pm_con_source.srl = srl_pm_con_source
left join pm_con_contract on pm_con_contract.srl = srl_con_contract
left join Pm_con_statusStatementtype on Pm_con_statusStatementtype .srl = srl_Statusstatementtype
left join pm_posttype on pm_posttype.srl = Srl_Pm_PostType
join Pm_Month on cast(Pm_Month.M as int ) = [Pm_Con_MonthReport].[MONTH] 
where 
(Pm_Con_MonthReport.Month in(SELECT *  FROM SPLIT(@Month , ',') )  or @Month='-1')
and (Pm_Con_MonthReport .Year = @year or @year =-1)
and (pm_con_contract.srl = @Srl_Contract or @Srl_Contract =-1)
and (Pm_Con_MonthReport .Srl_Pm_Vahed in(SELECT *  FROM SPLIT(@Srl_vahed, ',') ) or @srl_vahed='-1')
and (Pm_Con_MonthReport .srl_Statusstatementtype = @sstype or @sstype =-1)
and (Pm_Con_MonthReport .Srl_Pm_Con_source  in(SELECT *  FROM SPLIT(@srl_source , ',') ) or @srl_source='-1')
and (Pm_Con_MonthReport .Srl_Pm_PostType in(SELECT *  FROM SPLIT(@Srl_Omoor , ',') ) or @Srl_Omoor='-1')
and (@Status=1 or(@Status=2 and (ConfirmDate='' or ConfirmDate is null) and  (ConfirmPrice='' or ConfirmPrice  is null))
			   or(@Status=3 and ConfirmDate &lt;&gt;'' and ConfirmPrice&lt;&gt; '' and not ConfirmDate is null and not ConfirmPrice is null and (ERPDate is null or ERPDate ='' ))
			   or (@Status=4 and ConfirmDate &lt;&gt;'' and ConfirmPrice&lt;&gt; '' and not ConfirmDate is null and not ConfirmPrice is null and not ERPDate is null and  ERPDate &lt;&gt;'' )
			   )



</CommandText>

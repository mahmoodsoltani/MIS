                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT Pm_DarkhasteKar.[Srl] ,[Srl_Post] ,[Srl_TajhizForKar],[Srl_Vahed],[Srl_Group],[Srl_Sarparast],[Sharh_Kar] as  [شرح کار]  ,[Tozih] as [توضیحات]  ,[No] as [شماره] ,[Tarikh_Sabt] as [تاریخ ثبت],[No_Samak] as [شماره سامک], peyvasteT as [پیوسته],khamooshiT as [خاموشی], [Tarikh_From],[Tarikh_To],[Time_From],[Time_To]
      ,[Peyvaste],[Khamooshi],[Taeed_Cancel],[Status],Pm_DarkhasteKar.[Srl_SubmitUser],
      case when Pm_Hadese_KhesaratDevice.Srl_pm_TajhizName  IS null then
Pm_Hadese_KhesaratDevice.Name 
else
Pm_TajhizName.name   
end as [نام تحهیز],
Pm_Post.name as [نام پست],
pm_group.name as [نام گروه],
Pm_vahed.name as [نام واحد],
Pm_Ashkhas.Name + ' ' + Pm_Ashkhas.Family as [نام]

  FROM [Nama_MIS].[dbo].[Pm_DarkhasteKar] LEFT JOIN
  
   Pm_Hadese_KhesaratDevice ON Pm_Hadese_KhesaratDevice.srl = Srl_TajhizForKar
 LEFT JOIN Pm_TajhizName ON Pm_TajhizName .Srl = Pm_Hadese_KhesaratDevice.Srl_pm_TajhizName
 left join pm_post on Srl_Post = Pm_post.srl
left join pm_group on Srl_Group = pm_group.srl
left join pm_vahed on Srl_Vahed = pm_vahed.srl
left join Pm_Ashkhas on Srl_Sarparast = Pm_Ashkhas.Srl
where 
(Tarikh_Sabt&gt;=@StartSabt_Date OR @StartSabt_Date='' )AND
(Tarikh_Sabt&lt;=@EndSabt_Date OR @EndSabt_Date='')AND
(Tarikh_From&gt;=@Start_Date OR @Start_Date='' )AND
 (Tarikh_From&lt;=@End_Date OR @End_Date='')AND
(Srl_Post=@Srl_Post OR @Srl_Post=-1)AND
(Taeed_Cancel=@What OR @What=-1 )


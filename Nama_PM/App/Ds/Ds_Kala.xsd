          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Stg_KalaPriceTableAdapter" GeneratorDataComponentClassName="Stg_KalaPriceTableAdapter" Name="Stg_KalaPrice" UserDataComponentName="Stg_KalaPriceTableAdapter">

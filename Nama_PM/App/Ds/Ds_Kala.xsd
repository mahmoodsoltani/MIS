          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Stg_FactorDetailsTableAdapter" GeneratorDataComponentClassName="Stg_FactorDetailsTableAdapter" Name="Stg_FactorDetails" UserDataComponentName="Stg_FactorDetailsTableAdapter">

                    <CommandText>SELECT        Stg_KalaPrice.Srl, Stg_Kala.BarcodeCode AS بارکد, Stg_Kala.Name AS [نام کالا], ISNULL(Stg_FactorMaster.FactorNo, - 1) AS [شماره فاکتور خرید], 
                         ISNULL(Stg_FactorMaster.FactorDate, '') AS [تاریخ فاکتور خرید], ISNULL(Stg_FactorDetails.Price, 0) AS [قیمت خرید], ISNULL(Stg_KalaPrice.FroshPrice, 0) 
                         AS [قیمت فروش], Stg_KalaPrice.TarikhSabt AS [تاریخ ثبت], Stg_KalaPrice.Active AS فعال, Stg_Sazande.Name AS سازنده, Stg_Vahed.Name AS واحد, 
                         Stg_KalaGroup.Name AS [گروه کالا], Stg_Kala.Srl AS Srl_StgKala, Stg_KalaPrice.Srl_Stg_FactorMaster, Stg_KalaPrice.Srl_Stg_FactorDetails
						,isnull(AnbarCount.count,0) as [موجودی انبار]
FROM            Stg_FactorMaster INNER JOIN
                         Stg_FactorDetails ON Stg_FactorMaster.Srl = Stg_FactorDetails.Srl_Stg_FactorMaster RIGHT OUTER JOIN
                         Stg_KalaPrice INNER JOIN
                         Stg_Kala ON Stg_KalaPrice.Srl_Stg_Kala = Stg_Kala.Srl INNER JOIN
                         Stg_Vahed ON Stg_Kala.Srl_Stg_Vahed = Stg_Vahed.Srl INNER JOIN
                         Stg_KalaGroup ON Stg_Kala.Srl_Stg_KalaGroup = Stg_KalaGroup.Srl INNER JOIN
                         Stg_Sazande ON Stg_Kala.Srl_Stg_Sazande = Stg_Sazande.Srl ON Stg_FactorDetails.Srl = Stg_KalaPrice.Srl_Stg_FactorDetails
                         Left OUTER JOIN (select Srl_Stg_Kala,SUM(count)as count from Stg_AnbarKala  
                         Where( Srl_Stg_Anbar= @Srl_Anbar OR @Srl_Anbar=-1)  group by Srl_Stg_Kala)as AnbarCount 
                         ON Stg_Kala.Srl  = AnbarCount.Srl_Stg_Kala 
WHERE        (Stg_KalaPrice.Active = @Active) AND (Stg_Kala.Srl_Company = @Srl_Company OR @Srl_Company=-1) OR
                         (Stg_KalaPrice.Active IS NULL)
                         
</CommandText>

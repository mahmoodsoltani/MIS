                      <Parameter AllowDbNull="true" AutogeneratedName="Active" ColumnName="Active" DataSourceName="Nama_MIS.dbo.Stg_KalaPrice" DataTypeServer="bit" DbType="Boolean" Direction="Input" ParameterName="@Active" Precision="0" ProviderType="Bit" Scale="0" Size="1" SourceColumn="فعال" SourceColumnNullMapping="false" SourceVersion="Current" />

                                                 Pm_RecomendationSubject ON Pm_Post_Pishnahd.Srl_RecomSubject = Pm_RecomendationSubject.Srl INNER JOIN
                         Pm_Vahed ON Pm_Post_Pishnahd.Srl_Vahed = Pm_Vahed.Srl  LEFT JOIN

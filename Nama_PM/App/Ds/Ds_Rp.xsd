                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Pm_Post_Pishnahd.Pishnahad AS پیشنهاد, Pm_Post_Pishnahd.Tarikh AS تاریخ,Pm_Post_Pishnahd.No As شماره, Pm_Post_Pishnahd.No_Form as [شماره فرم], Pm_Post_Pishnahd.No_FormDo as [شماره فرم انجام کار], Pm_Post_Pishnahd.Tarikh_Do as [تاریخ انجام],Pm_Post_Pishnahd.Dis_AnjamKar as [شرح انجام کار], Pm_Post_Pishnahd.is_Do,Pm_Post_Pishnahd.PTime as بازه, Pm_Post_Pishnahd.is_NotDo, Pm_Post_Pishnahd.Status, Pm_Sender.Name as فرستنده,

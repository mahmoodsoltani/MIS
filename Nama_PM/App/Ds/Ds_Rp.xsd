          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Rp_GroupEibFormTableAdapter" GeneratorDataComponentClassName="Rp_GroupEibFormTableAdapter" Name="Rp_GroupEibForm" UserDataComponentName="Rp_GroupEibFormTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="Nama_PMConnectionString (MySettings)" DbObjectName="SELECT CASE WHEN t1.Srl_Group IS NULL THEN t2.Srl_Group ELSE t1.Srl_Group END AS [Srl_Group], ISNULL(t1.[رفع نشده],0) AS [رفع نشده], ISNULL(t2.[رفع شده] ,0)AS [رفع شده], ISNULL(ISNULL(t1.[رفع نشده],0) +ISNULL(t2.[رفع شده] ,0),0) AS [کل] FROM (SELECT ISnull(Srl_Pm_Group,-1)AS Srl_Group,COUNT(*) AS [رفع شده] FROM Pm_EibFormMaster WHERE (EibFixDate IS NOT NULL AND EibFixDate &lt;&gt; '' OR IsUnDecided =1 ) AND (OriginalEibFormDate&gt;=@EibFormFromDate OR @EibFormFromDate='-1') AND( (Pm_EibFormMaster.Srl NOT IN (SELECT Srl_Pm_EibFormMasterPeiro FROM Pm_EibFormPeiro)) OR (@ShowPeiro=-1)) AND (OriginalEibFormDate&lt;=@EibFormToDate OR @EibFormToDate='-1') AND (Srl_Pm_Group IN (SELECT * FROM split( @Group ,',')) OR @Group ='-1') GROUP BY Srl_Pm_Group )AS t2 FULL OUTER JOIN (SELECT ISNULL(Srl_Pm_Group,-1)AS Srl_Group,COUNT(*)AS [رفع نشده] FROM Pm_EibFormMaster WHERE ((EibFixDate IS NULL OR EibFixDate='') AND IsUnDecided &lt;&gt;1) AND (OriginalEibFormDate&gt;=@EibFormFromDate OR @EibFormFromDate='-1') AND( (Pm_EibFormMaster.Srl NOT IN (SELECT Srl_Pm_EibFormMasterPeiro FROM Pm_EibFormPeiro)) OR (@ShowPeiro=-1)) AND (OriginalEibFormDate&lt;=@EibFormToDate OR @EibFormToDate='-1') AND (Srl_Pm_Group IN (SELECT * FROM split( @Group ,',')) OR @Group ='-1') GROUP BY Srl_Pm_Group ) AS t1 ON t1.Srl_Group = t2.Srl_Group" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select isnull(Pm_Group.Name,'ناشناخته')as نام,t3.* from
(select case when t1.Srl_Group IS null then
	t2.Srl_Group 
	else
	t1.Srl_Group 
	end  as [Srl_Group],
ISNULL(t1.[رفع نشده],0) as [رفع نشده],
ISNULL(t2.[رفع شده] ,0)as [رفع شده],
ISNULL(ISNULL(t1.[رفع نشده],0) +ISNULL(t2.[رفع شده] ,0),0) as [کل]
 from 
(select ISnull(Srl_Pm_Group,-1)as Srl_Group,COUNT(*) as [رفع شده]
 from Pm_EibFormMaster 
 where  (EibFixDate  IS not  null AND EibFixDate &lt;&gt; '' OR IsUnDecided =1 )
 AND  (OriginalEibFormDate&gt;=@EibFormFromDate OR @EibFormFromDate='-1')
AND( (Pm_EibFormMaster.Srl not in (select Srl_Pm_EibFormMasterPeiro  from Pm_EibFormPeiro)) OR (@ShowPeiro=-1))
AND (OriginalEibFormDate&lt;=@EibFormToDate OR @EibFormToDate='-1')
AND (Srl_Pm_Group in (select * from split(@Group,',')) OR @Group='-1') 

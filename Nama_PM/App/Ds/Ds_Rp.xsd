            <MainSource>
              <DbSource ConnectionRef="Nama_PMConnectionString (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select ISNULL(Perform .x ,0) as [انجام شده],
ISNULL(WithDelay .x ,0) as [کنسل شده],
ISNULL(NoPerform .x ,0) as [انجام نشده],
ISNULL(Perform .x ,0)+ISNULL(WithDelay .x ,0)+ISNULL(NoPerform .x ,0) as [کل],

case when not Perform.[نام پست] IS null then Perform.[نام پست]
when not WithDelay.[نام پست] IS null then WithDelay.[نام پست]
when not NoPerform.[نام پست] IS null then NoPerform .[نام پست]
end  as [نام تجهیز]
 from 
(select COUNT(*) x,pm_post.Name as [نام پست],pm_post.Srl from 
pm_pmprogram JOIN Pm_Hadese_KhesaratDevice on
Srl_Pm_Hadese_KhesaratDevice =Pm_Hadese_KhesaratDevice.srl
Left JOIN Pm_Post on Pm_Hadese_KhesaratDevice .Srl_Pm_Post = pm_post.Srl 
LEFT JOIN Pm_TajhizName On Pm_TajhizName.Srl=Srl_pm_TajhizName 
 left join Pm_ExecutePmProgram on
Pm_PmProgram.Srl = Pm_ExecutePmProgram .Srl_Pm_PmProgram
where Pm_Date = ExecuteDate  and Pm_ExecutePmProgram.Srl  in (select MAX(srl)as srl from Pm_ExecutePmProgram 
	group by Srl_Pm_PmProgram)
	 and Pm_Date &gt;=@FromDate and Pm_Date&lt;=@ToDate 

Group By Pm_post.Srl,pm_post.Name having (pm_post.Srl in (select * from Split(@Post,',')) or @Post='')  
  )as Perform
Full Join
(select COUNT(*)x,pm_post.Name as [نام پست],pm_post.Srl from 
pm_pmprogram JOIN Pm_Hadese_KhesaratDevice on
Srl_Pm_Hadese_KhesaratDevice =Pm_Hadese_KhesaratDevice.srl
Left JOIN Pm_Post on Pm_Hadese_KhesaratDevice .Srl_Pm_Post = pm_post.Srl 
LEFT JOIN Pm_TajhizName On Pm_TajhizName.Srl=Srl_pm_TajhizName 

          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Pm_Con_StatusstatementItemVwTableAdapter" GeneratorDataComponentClassName="Pm_Con_StatusstatementItemVwTableAdapter" Name="Pm_Con_StatusstatementItemVw" UserDataComponentName="Pm_Con_StatusstatementItemVwTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="SELECT i.*,Pm_Con_Item.title,pm_con_item.srl AS srl_item, CASE WHEN NOT srl_line IS NULL THEN pm_line.name WHEN NOT Srl_workform_master IS NULL THEN pworkform.name ELSE pstatus.name END AS postname, CASE WHEN Srl_workform_master IS NULL THEN pm_con_statusstatement.tarikh ELSE pm_workform_master.workformtarikh END AS tarikh , CASE WHEN Is_Holiday =1 THEN round(i .Price*i .Amount *Holiday,0) WHEN is_night=1 THEN round(i .Price*i .Amount *NightWork,0) WHEN Is_Cancle =1 THEN CASE WHEN NOT Srl_workform_master IS NULL THEN round(i .Price*i .Amount *WorkForm_Cancle,0) ELSE round(i .Price*i .Amount *NonWorkForm_Cancle,0) END WHEN Is_BeforeCancle =1 THEN CASE WHEN NOT Srl_workform_master IS NULL THEN round(i .Price*i .Amount *WorkForm_Cancle_BeforDay,0) ELSE round(i .Price*i .Amount * NonWorkForm_Cancle_BeforeDay,0) END ELSE round(i.Price * i.Amount, 0) END AS p, CASE WHEN i.Srl_workform_master IS NULL THEN Pm_Con_StatusStatement.Srl_vahed ELSE Pm_WorkForm_Master.Srl_Pm_Vahed END AS srl_vahed, CASE WHEN i.Srl_workform_master IS NULL THEN CASE WHEN Pm_Con_StatusStatement .Srl_post IS NULL THEN Pm_Con_StatusStatement.Srl_Line ELSE Pm_Con_StatusStatement.Srl_post END ELSE Pm_WorkForm_Master.Srl_Pm_Post_From END AS srl_Post, CASE WHEN i.Srl_workform_master IS NULL THEN Pm_Con_StatusStatement.Srl_group ELSE Pm_WorkForm_Master.Srl_Pm_Group END AS srl_group, CASE WHEN NOT PDStatus .srl IS NULL THEN PDStatus.Srl_Pm_PostType WHEN NOT PDWorkForm.srl IS NULL THEN PDWorkForm.srl_pm_posttype ELSE Pm_Line.Srl_Pm_PostType END AS srl_Omoor, CASE WHEN NOT Pm_Con_StatusStatement .Tarikh IS NULL THEN CAST(SUBSTRING(Pm_Con_StatusStatement.Tarikh,6,2)AS int ) ELSE CAST(SUBSTRING(Pm_WorkForm_Master.workformtarikh,6,2)AS int )END AS _Month , CASE WHEN NOT Pm_Con_StatusStatement .Tarikh IS NULL THEN CAST(SUBSTRING(Pm_Con_StatusStatement.Tarikh,1,4)AS int ) ELSE CAST(SUBSTRING(Pm_WorkForm_Master.workformtarikh,1,4)AS int ) END AS _Year , sstype FROM Pm_Con_StatusStatementItem AS i LEFT JOIN Pm_Con_StatusStatement ON Pm_Con_StatusStatement .Srl = Srl_StatusStatement LEFT JOIN Pm_WorkForm_Master ON Pm_WorkForm_Master .srl =Srl_workform_master LEFT JOIN Pm_Post AS PStatus ON Pm_Con_StatusStatement .Srl_post =PStatus.Srl LEFT JOIN Pm_PostDetails PDStatus ON PStatus.Srl = PDStatus.Srl_Pm_Post LEFT JOIN Pm_Con_ContractItems ON Pm_Con_ContractItems.Srl = srl_contractItem LEFT JOIN Pm_Con_Item ON pm_con_item.Srl = Pm_Con_ContractItems.Srl_Item LEFT JOIN Pm_Post AS pWorkForm ON Pm_WorkForm_Master .Srl_Pm_Post_From =pWorkForm.Srl LEFT JOIN Pm_PostDetails PDWorkForm ON pWorkForm.Srl = PDWorkForm.Srl_Pm_Post LEFT JOIN Pm_Line ON Srl_Line = Pm_Line .Srl LEFT JOIN (SELECT * FROM Pm_Con_Contractzarib)AS m ON m.Srl_Contract = Pm_Con_ContractItems.Srl_Contract" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select t.title as عنوان,t.amount as تعداد, t.price as قیمت,
t.p as کل , t.tarikh as تاریخ, t.postname as پست ,srl_Post,t.* from 
(  select i.*,Pm_Con_Item.title,pm_con_item.srl as srl_item,
case when not srl_line is null then  pm_line.name
	when  not Srl_workform_master is null then  pworkform.name 
	else pstatus.name end  as postname,
case when  Srl_workform_master is null then pm_con_statusstatement.tarikh
	 else
	 pm_workform_master.workformtarikh end as tarikh , 
case when Is_Holiday =1 then round(i .Price*i .Amount *Holiday,0)
when is_night=1 then round(i .Price*i .Amount *NightWork,0)
when Is_Cancle =1 then
case when not Srl_workform_master is null then round(i .Price*i .Amount *WorkForm_Cancle,0)
else round(i .Price*i .Amount *NonWorkForm_Cancle,0)
End 	when Is_BeforeCancle =1 then
 case when not Srl_workform_master is null then round(i .Price*i .Amount *WorkForm_Cancle_BeforDay,0)
else round(i .Price*i .Amount * NonWorkForm_Cancle_BeforeDay,0)
End 	
else round(i.Price * i.Amount, 0) End as p,
 case when i.Srl_workform_master  is null then Pm_Con_StatusStatement.Srl_vahed 
 else Pm_WorkForm_Master.Srl_Pm_Vahed End as srl_vahed,
  case when i.Srl_workform_master  is null then 
	case when Pm_Con_StatusStatement .Srl_post IS null then Pm_Con_StatusStatement.Srl_Line 
	else Pm_Con_StatusStatement.Srl_post End else  Pm_WorkForm_Master.Srl_Pm_Post_From End as srl_Post,
	case when i.Srl_workform_master  is null then Pm_Con_StatusStatement.Srl_group 
	else Pm_WorkForm_Master.Srl_Pm_Group End as srl_group,
	 case	when not PDStatus .srl is null then PDStatus.Srl_Pm_PostType 
			when not  PDWorkForm.srl is null then PDWorkForm.srl_pm_posttype
			else Pm_Line.Srl_Pm_PostType End as srl_Omoor, 
	case when not Pm_Con_StatusStatement .Tarikh IS null then	CAST(SUBSTRING(Pm_Con_StatusStatement.Tarikh,6,2)As int )
		else CAST(SUBSTRING(Pm_WorkForm_Master.workformtarikh,6,2)as int )end as _Month ,
			case when not Pm_Con_StatusStatement .Tarikh IS null then	CAST(SUBSTRING(Pm_Con_StatusStatement.Tarikh,1,4)As int )
			else CAST(SUBSTRING(Pm_WorkForm_Master.workformtarikh,1,4)as int )	end as _Year , sstype
from Pm_Con_StatusStatementItem as i left join Pm_Con_StatusStatement on Pm_Con_StatusStatement .Srl = Srl_StatusStatement 
 left join Pm_WorkForm_Master on Pm_WorkForm_Master .srl =Srl_workform_master  
 left join Pm_Post as PStatus on Pm_Con_StatusStatement  .Srl_post  =PStatus.Srl 
  left join Pm_PostDetails PDStatus on PStatus.Srl  = PDStatus.Srl_Pm_Post
   left join Pm_Con_ContractItems  on Pm_Con_ContractItems.Srl = srl_contractItem  
   left join Pm_Con_Item on pm_con_item.Srl = Pm_Con_ContractItems.Srl_Item  
   left join Pm_Post as pWorkForm on Pm_WorkForm_Master .Srl_Pm_Post_From =pWorkForm.Srl  
   left join Pm_PostDetails PDWorkForm on pWorkForm.Srl  = PDWorkForm.Srl_Pm_Post 

          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="Pm_Con_MonthReportAmount" GeneratorDataComponentClassName="Pm_Con_MonthReportAmount" Name="Pm_Con_MonthReportAmount" UserDataComponentName="Pm_Con_MonthReportAmount">
            <MainSource>
              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select isnull(SUM(p),0) from (  select Pm_Con_StatusStatementItem.*,
            case when Is_Holiday =1 then
             round(Pm_Con_StatusStatementItem .Price*Pm_Con_StatusStatementItem .Amount *Holiday,0)
            when is_night=1 then round(Pm_Con_StatusStatementItem .Price*Pm_Con_StatusStatementItem .Amount *NightWork,0)
            when Is_Cancle =1 then case when not Srl_workform_master is null then round(Pm_Con_StatusStatementItem .Price*Pm_Con_StatusStatementItem .Amount *WorkForm_Cancle,0)
else round(Pm_Con_StatusStatementItem .Price*Pm_Con_StatusStatementItem .Amount *NonWorkForm_Cancle,0)
            End 	when Is_BeforeCancle =1 then case when not Srl_workform_master is null then round(Pm_Con_StatusStatementItem .Price*Pm_Con_StatusStatementItem .Amount *WorkForm_Cancle_BeforDay,0)
else round(Pm_Con_StatusStatementItem .Price*Pm_Con_StatusStatementItem .Amount * NonWorkForm_Cancle_BeforeDay,0)
End else round(Pm_Con_StatusStatementItem.Price * Pm_Con_StatusStatementItem.Amount, 0) End as p,
case when Pm_Con_StatusStatementItem.Srl_workform_master  is null then Pm_Con_StatusStatement.Srl_vahed 
else Pm_WorkForm_Master.Srl_Pm_Vahed End as srl_vahed,
case when Pm_Con_StatusStatementItem.Srl_workform_master  is null then 
case when Pm_Con_StatusStatement .Srl_post IS null then Pm_Con_StatusStatement.Srl_Line 
else Pm_Con_StatusStatement.Srl_post End else  Pm_WorkForm_Master.Srl_Pm_Post_From End as srl_Post,case when Pm_Con_StatusStatementItem.Srl_workform_master  is null then Pm_Con_StatusStatement.Srl_group 
else Pm_WorkForm_Master.Srl_Pm_Group End as srl_group,
case	when not PDStatus .srl is null then PDStatus.Srl_Pm_PostType 
when not  PDWorkForm.srl is null then PDWorkForm.srl_pm_posttype
else Pm_Line.Srl_Pm_PostType End as srl_Omoor, 
case when not Pm_Con_StatusStatement .Tarikh IS null then	CAST(SUBSTRING(Pm_Con_StatusStatement.Tarikh,6,2)As int )
else CAST(SUBSTRING(Pm_WorkForm_Master.WorkFormTarikh ,6,2)as int )end as _Month ,
case when not Pm_Con_StatusStatement .Tarikh IS null then	CAST(SUBSTRING(Pm_Con_StatusStatement.Tarikh,1,4)As int )
else CAST(SUBSTRING(Pm_WorkForm_Master.WorkFormTarikh,1,4)as int )	end as _Year , sstype
from Pm_Con_StatusStatementItem left join Pm_Con_StatusStatement on Pm_Con_StatusStatement .Srl = Srl_StatusStatement 
left join Pm_WorkForm_Master on Pm_WorkForm_Master .srl =Srl_workform_master  
left join Pm_Post as PStatus on Pm_Con_StatusStatement  .Srl_post  =PStatus.Srl 
left join Pm_PostDetails PDStatus on PStatus.Srl  = PDStatus.Srl_Pm_Post
left join Pm_Con_ContractItems  on Pm_Con_ContractItems.Srl = srl_contractItem  
left join Pm_Con_Item on pm_con_item.Srl = Pm_Con_ContractItems.Srl_Item  
left join Pm_Post as pWorkForm on Pm_WorkForm_Master .Srl_Pm_Post_From =pWorkForm.Srl  
left join Pm_PostDetails PDWorkForm on pWorkForm.Srl  = PDWorkForm.Srl_Pm_Post 
left join Pm_Line on Srl_Line = Pm_Line .Srl 
left join (select * from Pm_Con_Contractzarib)as m on m.Srl_Contract = Pm_Con_ContractItems.Srl_Contract ) t where  (t.srl_Omoor=@Srl_Omoor Or  @Srl_Omoor =-1)and
           ( t.srl_vahed = @Srl_vahed or @Srl_vahed=-1)  and
           (Srl_Source =@srl_Source or @srl_Source=-1) and
           (_Year = @Year or @Year=-1) and
           (_Month = @Month or @Month=-1)   and 
